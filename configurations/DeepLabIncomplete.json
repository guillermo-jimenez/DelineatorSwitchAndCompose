                "class": "utils.torch.nn.SeparableConv1d",
                "arguments": {
                    "dilation": 12,
                    "padding": 12,
                    "groups": 1
                }
            },
            {
                "id": "atrous18",
                "class": "utils.torch.nn.SeparableConv1d",
                "arguments": {
                    "dilation": 18,
                    "padding": 18,
                    "groups": 1
                }
            },
            {
                "id": "atrouspool",
