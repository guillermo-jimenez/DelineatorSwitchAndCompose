                        },
                        {
                            "class": "utils.torch.nn.Dropout1d",
                            "arguments": {
                                "p": 0.25
                            }
                        }
                    ]
                }
            },
            {
                "id": "encoder_avgpool3",
                "class": "torch.nn.AvgPool1d",
                "arguments": {
                    "kernel_size": 2
                }
            },
            {
                "id": "encoder_lvl4",
                "class": "utils.torch.nn.CNN",
                "operation": {
                    "class": "utils.torch.nn.SeparableConv1d"
                },
                "arguments": {
                    "channels": [
                        32,
                        64,
                        64
                    ],
                    "regularization": [
                        {
                            "class": "torch.nn.LeakyReLU"
                        },
                        {
                            "class": "torch.nn.BatchNorm1d",
                            "arguments": {
                                "num_features": "update"
                            }
                        },
                        {
                            "class": "utils.torch.nn.Dropout1d",
                            "arguments": {
                                "p": 0.25
                            }
                        }
                    ]
                }
            },
            {
                "id": "atrous1",
                "class": "utils.torch.nn.SeparableConv1d",
                "arguments": {
                    "dilation": 1,
                    "padding": 1,
                    "groups": 1
