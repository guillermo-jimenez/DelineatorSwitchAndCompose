{
    "root_directory": "~/DADES/DADES/EjectionFraction/Databases/",
    "save_directory": "~/DADES/DADES/EjectionFraction/TrainedModels/",
    "device": "cuda",
    "seed": 123456,
    "resume": false,
    "input_shape": 1024,
    "iterator": "tqdm.tqdm",
    "epochs": 500,
    "sampler": {
        "shuffle": true
    },
    "loader": {
        "batch_size": 2048,
        "num_workers": 0,
        "pin_memory": true
    },
    "optimizer": {
        "class": "torch.optim.Adam",
        "arguments": {
            "lr": 0.001
        }
    },
    "scheduler": {
        "class": "torch.optim.lr_scheduler.ReduceLROnPlateau",
        "arguments": {
            "factor": 0.1,
            "patience": 100
        }
    },
    "augmentation": {
        "class": "torchvision.transforms.RandomChoice",
        "arguments": {},
        "types": {
            "utils.torch.data.augmentation.AdditiveWhiteGaussianNoise": [
                20,
                0.01
            ],
            "utils.torch.data.augmentation.PowerlineNoise": [
                20,
                500,
                50.0
            ],
            "utils.torch.data.augmentation.BaselineNoise": [
                -5,
                500,
                0.5
            ],
            "utils.torch.data.augmentation.ChangeAmplitude": [
                0.01
            ],
            "utils.torch.data.augmentation.SegmentationErrors": [
                10
            ]
        }
    },
    "loss": [
        {
            "class": "torch.nn.MSELoss",
            "arguments": {
                "reduction": "mean"
            },
            "mapping": [
                "output_3",
                "input_0"
            ],
            "weight": 5
        },
        {
            "class": "KLDivergence",
            "arguments": {},
            "mapping": [
                "output_0",
                "output_1"
            ],
            "weight": 0.01
        }
    ],
    "model": {
        "type": "graph",
        "functions": [
            {
                "name": "forward",
                "inputs": [
                    "encoder_lvl1"
                ],
                "outputs": [
                    "decoder_lvl1"
                ]
            }
        ],
        "edges": [
            ["encoder_lvl1", "encoder_avgpool1"],
            ["encoder_avgpool1", "encoder_lvl2"],
            ["encoder_lvl2", "encoder_avgpool2"],
            ["encoder_avgpool2", "encoder_lvl3"],
            ["encoder_lvl3", "decoder_upsample3"],
            ["decoder_upsample3", ["decoder_upsample3","encoder_lvl2"]],
            ["encoder_lvl2", ["decoder_upsample3","encoder_lvl2"]],
            [["decoder_upsample3","encoder_lvl2"],"decoder_concat2"],
            ["decoder_concat2", "decoder_lvl2"],
            ["decoder_lvl2", "decoder_upsample2"],
            ["decoder_upsample2", ["decoder_upsample2","encoder_lvl1"]],
            ["encoder_lvl1", ["decoder_upsample2","encoder_lvl1"]],
            [["decoder_upsample2","encoder_lvl1"],"decoder_concat1"],
            ["decoder_concat1", "decoder_lvl1"]
        ],
        "nodes": [
            {
                "id": "encoder_lvl1",
                "class": "utils.torch.nn.CNN",
                "operation": {
                    "class": "utils.torch.nn.SeparableConv1d"
                },
                "arguments": {
                    "channels": [
                        1,
                        16,
                        16
                    ],
                    "regularization": [
                        {
                            "class": "torch.nn.LeakyReLU"
                        },
                        {
                            "class": "torch.nn.BatchNorm1d",
                            "arguments": {
                                "num_features": "update"
                            }
                        },
                        {
                            "class": "utils.torch.nn.Dropout1d",
                            "arguments": {
                                "p": 0.25
                            }
                        }
                    ]
                }
            },
            {
                "id": "encoder_avgpool1",
                "class": "torch.nn.AvgPool1d",
                "arguments": {
                    "kernel_size": 2
                }
            },
            {
                "id": "encoder_lvl2",
                "class": "utils.torch.nn.CNN",
                "operation": {
                    "class": "utils.torch.nn.SeparableConv1d"
                },
                "arguments": {
                    "channels": [
                        16,
                        32,
                        32
                    ],
                    "regularization": [
                        {
                            "class": "torch.nn.LeakyReLU"
                        },
                        {
                            "class": "torch.nn.BatchNorm1d",
                            "arguments": {
                                "num_features": "update"
                            }
                        },
                        {
                            "class": "utils.torch.nn.Dropout1d",
                            "arguments": {
                                "p": 0.25
                            }
                        }
                    ]
                }
            },
            {
                "id": "encoder_avgpool2",
                "class": "torch.nn.AvgPool1d",
                "arguments": {
                    "kernel_size": 2
                }
            },
            {
                "id": "encoder_lvl3",
                "class": "utils.torch.nn.CNN",
                "operation": {
                    "class": "utils.torch.nn.SeparableConv1d"
                },
                "arguments": {
                    "channels": [
                        32,
